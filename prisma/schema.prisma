generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum etatCommande{
  enCour
  enAttente
  terminee
  livree
}

enum morphologie{
  enfant
  femme
  homme
}

enum utilisateur{
  admin
  employe
  client
}

model User{
  id string @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  nom string 

  @@map("user")
}


